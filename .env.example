# Event Gallery Environment Variables
# Copy this file to .env and update with your actual values

# Database Configuration
DB_URL=jdbc:postgresql://localhost:5432/event_gallery_db
DB_USERNAME=event_gallery_user
DB_PASSWORD=your_secure_password_here

# AWS S3 Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_S3_BUCKET=event-gallery-images-unique-suffix
AWS_S3_REGION=us-east-1

# JWT Configuration
# Generate a secure random string for production
# Minimum 256 bits (32 characters)
JWT_SECRET=your_very_long_and_secure_jwt_secret_key_minimum_256_bits

# Application Configuration
APP_BASE_URL=http://localhost:8080
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:19006

# Frontend Configuration (for frontend/.env)
REACT_APP_API_URL=http://localhost:8080/api
